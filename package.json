    "dependencies": {
        "axios": "^0.21.1",
        "bcryptjs": "^2.4.3",
        "bootstrap": "^5.0.2",
        "colors": "^1.4.0",
        "cors": "^2.8.5",
        "dotenv": "^10.0.0",
        "express": "^4.17.1",
        "express-async-handler": "^1.1.4",
        "jsonwebtoken": "^8.5.1",
        "mogoose": "^0.0.1-security",
        "mongoose": "^5.13.3",
        "morgan": "^1.10.0",
        "multer": "^1.4.2",
